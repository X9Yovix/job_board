		@import url("https://fonts.googleapis.com/css?family=Catamaran:400, 600|IBM+Plex+Sans:400, 500, 500i, 600, 700 &display=swap");

		body {
			font-family: "IBM Plex Sans", sans-serif;
			font-size: 17px;
			width: 100%;
			background-color: #383434;
			margin: 0;
			padding: 0;
			-webkit-font-smoothing: antialiased;
			mso-margin-top-alt: 0;
			mso-margin-bottom-alt: 0;
			mso-padding-alt: 0 0 0 0;
		}

		p,
		h1,
		h2,
		h3,
		h4 {
			margin-top: 0;
			margin-bottom: 0;
			padding-top: 0;
			padding-bottom: 0;
		}

		span.preheader {
			display: none;
			font-size: 1px;
		}

		html {
			width: 100%;
		}

		table {
			font-size: 12px;
			border: 0;
		}

		.menu-space {
			padding-right: 25px;
		}

		a,
		a:hover {
			text-decoration: none;
			color: #FFF;
		}

		@media only screen and(max-width:640px) {
			body {
				width: auto !important;
			}

			table [class=main] {
				width: 440px !important;
			}

			table [class=two-left] {
				width: 420px !important;
				margin: 0 auto;
			}

			table [class=full] {
				width: 100% !important;
				margin: 0 auto;
			}

			table [class=two-left-inner] {
				width: 400px !important;
				margin: 0 auto;
			}

			table [class=menu-icon] {
				display: none;
			}
		}

		@media only screen and(max-width:479px) {
			body {
				width: auto !important;
			}

			table [class=main] {
				width: 310px !important;
			}

			table [class=two-left] {
				width: 300px !important;
				margin: 0 auto;
			}

			table [class=full] {
				width: 100% !important;
				margin: 0 auto;
			}

			table [class=two-left-inner] {
				width: 280px !important;
				margin: 0 auto;
			}

			table [class=menu-icon] {
				display: none;
			}
		}
	</style>
</head>

